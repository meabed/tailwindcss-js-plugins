  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "source": "src/index.js",
  "maintainers": [
    {
      "email": "mo@meabed.com",
      "name": "Mohamed Meabed",
      "url": "https://twitter.com/meabed"
    }
