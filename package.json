    "tailwindcss",
    "tailwind",
    "tailwind-plugins",
    "tailwind-button",
    "tailwind-form",
    "tailwind-grid"
  ],
  "author": {
    "email": "mo@meabed.com",
    "name": "Mohamed Meabed",
    "url": "https://twitter.com/meabed"
  },
